<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.testOther.TextCircleViewPager">

    <com.seaway.android.common.widget.circlevp.CircleViewPager
        android:id="@+id/text3_page"
        android:layout_width="match_parent"
        android:layout_height="165dp"
         />

    <com.seaway.android.common.widget.circlevp.CircleIndicator
        android:id="@+id/text3_indicator"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="140dp"
        android:padding="5dp"
        android:visibility="gone"
        app:circlePadding="3dp"
        app:fillColor="#ffffff"
        app:pageColor="#4fffffff"
        app:radius="4.0dp"
        app:strokeColor="#000000"
        app:strokeWidth="0dp" />

</FrameLayout >
